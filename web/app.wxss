/**app.wxss**/
@import 'weui.wxss';
page {
  background: #EDF8FE;
}
.header {
  height: 80rpx;
  line-height: 80rpx;
  width: 100%;
  text-align: center;
  background: #1e1e1e;
  color: #fff;
  font-size: 34rpx;
  position: fixed;
  top:0;
  z-index: 10;
}
.all_container{
  margin-top: 80rpx;
}
.go_back {
  display: inline;
  position: fixed;
  left: 0;
  font-size: 26rpx;
}

.go_back image {
  width: 35rpx;
  height: 35rpx;
  vertical-align: middle;
  margin-top: -3rpx;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
button{
  background-color: #1396F1;
  color: #fff;
}
.weui-cell__title{
   font-size: 36rpx;
}
.weui-cell__bd{
  font-size: 32rpx;
}
